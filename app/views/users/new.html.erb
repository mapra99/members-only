<h1>Sign Up</h1>

<%= form_for(@user, url: users_create_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br><br>
  <%= f.submit "Sign Up" %>
<% end %>